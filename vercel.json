{
    "name": "deploy-node-in-vercel",
    "version": "2",
    "public": "true",
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "server/server.js"
        }
    ]
}